    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#affects -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#affects">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isAffectedBy"/>
        <rdfs:comment xml:lang="en">This property points to an instance of Role who is affected by the current function (e.g. the role performed by an actress)</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">affects</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#authorEducationLevel">
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:comment xml:lang="en">Level of education of the creator of a text/song (this property has as Domain &quot;PoeticWork&quot; and not &quot;Person&quot; since the education level of a person changes during time. This is the education level of the author in the moment of writing the poem)</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">authorEducationLevel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#belongsToListOrderedText -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#belongsToListOrderedText">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/olo/core#ordered_list"/>
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#hasTextUnit"/>
        <rdfs:comment xml:lang="en">The text unit belongs an ordered list of a textual units</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">belongsToListOrderedText</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#birthDate -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#birthDate">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/birthDate"/>
        <rdfs:comment xml:lang="en">Date of birth</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birthDate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#birthPlace -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#birthPlace">
        <rdfs:subPropertyOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isFrom"/>
        <rdfs:comment xml:lang="en">This property points to the Place which is the provenance of the current instance of Person</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birthPlace</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceCertainty -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#birthPlaceCertainty">
        <rdfs:subPropertyOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#certainty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/certainty"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Certainty of the date of birth</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birthPlaceCertainty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#certainty -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#certainty">
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/certainty"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Level of confidence regarding the correspondent function</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">certainty</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#creates -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#creates">
        <rdfs:comment xml:lang="en">This property points to an ensemble, PoeticWork or Redaction which was authored by the current Creator</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">creates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#date -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#date">
        <rdfs:comment xml:lang="en">This property points to an date entity that represents a particular point or period of time at which something happened or existed, or is expected to happen</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">date</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#deathDate -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#deathDate">
        <rdfs:subPropertyOf rdf:resource="http://schema.org/deathDate"/>
        <rdfs:comment xml:lang="en">Date of death</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">deathDate</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#diedIn -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#diedIn">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isDeathPlace"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Place"/>
        <rdfs:comment xml:lang="en">This property points to the instance of Place where the current Person has died</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">diedIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#floruit -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#floruit">
        <rdfs:comment xml:lang="en">A person’s period of activity (to use when a range cannot be stablished; eg. “3rd century”)</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">floruit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#gender -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#gender">
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/gender"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The gender of the Person</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">gender</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#genre -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#genre">
        <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/genre"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Type of art, literature or music characterized by a specific form, content and style</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">genre</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasAgent -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasAgent">
        <rdfs:comment xml:lang="en">This property points to a function (an instance of Role) that has somehow affected the item at hand (e.g. sponsor)</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasAgent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasCharacter -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasCharacter">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">This property points to a group of characters, an instance of Agent( Organisation or Person) depicted in the text</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasCharacter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasCreator -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasCreator">
        <rdfs:comment xml:lang="en">This property points to the entity Creator that has created the current work under analysis</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasCreator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasDedication -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasDedication">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isDedicatedTo"/>
        <rdfs:range rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">This property points to the text (an instance of Redaction) which was dedicated to the current Person</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasDedication</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasEditor -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasEditor">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isEditor"/>
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">This property points to the Person who has edited the current instance of Redaction</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasEditor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasFirstPart -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasFirstPart">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isFirstPart"/>
        <rdfs:comment xml:lang="en">This property points to the first text unit  which form the current element under analysis</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasFirstPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasMember -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasMember">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isMember"/>
        <rdfs:comment xml:lang="en">This property points to one of the persons (instance of Person) who belongs to the current Organisation</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasMember</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasOrderedTextList -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasOrderedTextList">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">List of  text units that form the redaction</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasOrderedTextList</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasPart -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasPart">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart"/>
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isPart"/>
        <rdfs:comment xml:lang="en">This property points to one of the works (an instance of PoeticWork) which form the current element under analysis</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasTextUnit -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasTextUnit">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/olo/core#slot"/>
        <rdfs:comment xml:lang="en">A text unit in an ordered list.</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasTextUnit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#hasTranslation -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#hasTranslation">
        <rdfs:domain rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction"/>
        <rdfs:comment xml:lang="en">This property points to the specific location in a bibliographic item (an instance of Location) which contains a translation of the text at hand</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasTranslation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#involves -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#involves">
        <owl:inverseOf rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#isInvolvedIn"/>
        <rdfs:comment xml:lang="en">This property points to a Organisation, Person or Place  which are related to the current instance of Event</rdfs:comment>
        <rdfs:isDefinedBy>http://postdata.linhd.uned.es/ontology/postdata-core/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">involves</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#isAffectedBy -->

    <owl:ObjectProperty rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#isAffectedBy">
