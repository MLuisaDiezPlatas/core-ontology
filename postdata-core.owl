    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#Redaction -->

    <owl:Class rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#Redaction">
        <rdfs:subClassOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F22_Self-Contained_Expression"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://postdata.linhd.uned.es/ontology/postdata-core#genre"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                                <owl:hasValue rdf:resource="http://postdata.linhd.uned.es/kos/genre"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
