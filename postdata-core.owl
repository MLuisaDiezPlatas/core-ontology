    <!-- http://postdata.linhd.uned.es/ontology/postdata-core#Place -->

    <owl:Class rdf:about="http://postdata.linhd.uned.es/ontology/postdata-core#Place">
        <rdfs:subClassOf rdf:resource="http://schema.org/Place"/>
